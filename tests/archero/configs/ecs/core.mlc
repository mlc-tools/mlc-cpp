class ecs/Math
{

    fn float ease_back_in(float t):static
    {
        const float c1 = 1.70158f;
        return (c1 + 1) * t * t * t - c1 * t * t;
    }
    fn float ease_back_out(float t):static
    {
        const float c1 = 1.70158f;
        return 1 + (c1 + 1.0f) * std::pow(t - 1, 3) + c1 * std::pow(t - 1, 2);
    }

    fn float ease_in_out(float t):static
    {
        return -(std::cos(M_PI * t) - 1) / 2;
    }


}